# connect to postgre server first time.
psql -d postgres --username=pgsql

# set the password of ther pgsql
\password pgsql

# list the databases in the system.
\list

#list the tables in the current database.
\dt

#list the table definition
\d tablename

#make a component
./makecomponent AngularTestController angular

#access url
http://192.168.1.5/fcgi/manage/list/
